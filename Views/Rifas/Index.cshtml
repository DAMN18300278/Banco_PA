@model IEnumerable<Banco.Models.Rifa>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.FechaBoleto)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CuentaNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.FechaBoleto)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CuentaNavigation.NumCuenta)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.NumBoleto">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.NumBoleto">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.NumBoleto">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
